# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/node_modules
/yarn-error.log
config/application.yml

.byebug_history

# Ignore public uploads
/public/packs
/public/packs-test
/public/uploads


/node_modules
yarn-debug.log*
.yarn-integrity

.ruby-version
.ruby-gemset


# Ignore application configuration
/config/application.yml
